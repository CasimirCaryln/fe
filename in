void nhap()
{
	int i, j, t;
	for (i = 0; i < n; i++)
	{
		cout << "Nhap cac phan tu hang thu " << i + 1 << ":\n";
		for (j = 0; j < n; j++)
		{
			cout << "A[" << i + 1 << "," << j + 1 << "]=";
			cin >> t;
			a[i][j] = t;
		}
	}
